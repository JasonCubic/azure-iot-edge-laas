{
  "$schema-template": "1.0.0",
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {}
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.0",
              "createOptions": {}
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.0",
              "createOptions": {
                "HostConfig": {
                  "PortBindings": {
                    "5671/tcp": [
                      {
                        "HostPort": "5671"
                      }
                    ],
                    "8883/tcp": [
                      {
                        "HostPort": "8883"
                      }
                    ],
                    "443/tcp": [
                      {
                        "HostPort": "443"
                      }
                    ]
                  }
                }
              }
            }
          }
        },
        "modules": {
          "tempSensor": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-simulated-temperature-sensor:1.0",
              "createOptions": {}
            }
          },
          "filtermodule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.filtermodule.debug}",
              "createOptions": {}
            }
          },
          "laas": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.laas.debug}",
              "createOptions": {
                "ExposedPorts": {
                  "9229/tcp": {}
                },
                "HostConfig": {
                  "PortBindings": {
                    "9229/tcp": [
                      {
                        "HostPort": "9229"
                      }
                    ]
                  }
                }
              }
            }
          },
          "rfidReader": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.rfidReader.debug}",
              "createOptions": {
                "ExposedPorts": {
                  "9229/tcp": {}
                },
                "HostConfig": {
                  "PortBindings": {
                    "9229/tcp": [
                      {
                        "HostPort": "9229"
                      }
                    ]
                  }
                }
              }
            }
          },
          "zebraFxConnectListener": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.zebraFxConnectListener.debug}",
              "createOptions": {
                "ExposedPorts": {
                  "9229/tcp": {}
                },
                "HostConfig": {
                  "PortBindings": {
                    "9229/tcp": [
                      {
                        "HostPort": "9229"
                      }
                    ]
                  }
                }
              }
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "sensorTofiltermodule": "FROM /messages/modules/tempSensor/outputs/temperatureOutput INTO BrokeredEndpoint(\"/modules/filtermodule/inputs/input1\")",
          "filtermoduleToIoTHub": "FROM /messages/modules/filtermodule/outputs/* INTO $upstream",
          "laasToIoTHub": "FROM /messages/modules/laas/outputs/* INTO $upstream",
          "rfidReaderToIoTHub": "FROM /messages/modules/rfidReader/outputs/* INTO $upstream",
          "zebraFxConnectListenerToIoTHub": "FROM /messages/modules/zebraFxConnectListener/outputs/* INTO $upstream"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    }
  }
}